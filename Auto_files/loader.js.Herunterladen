(function(){'use strict';var a={apiBaseUrl:"https://www.mobile.de/api/consent"};const b=(a,c=1,d=1)=>new Promise(function(e,f){fetch(a).then(e).catch(g=>1===c?f(g):void setTimeout(()=>{e(b(a,c-1,2*d))},d))});(()=>b(`${a.apiBaseUrl}/static/js/scripts`,5,500).then(a=>a.ok?a.text():Promise.reject({body:JSON.stringify({status:a.status,statusText:a.statusText}),type:"status not ok"})).then(a=>{try{return JSON.parse(a)}catch(b){return Promise.reject({body:a,type:"unparsable"})}}).then(({api:a})=>a))().then(a=>{const b=document.createElement("script");b.setAttribute("src",a),b.setAttribute("defer","defer"),document.body.appendChild(b)}).catch(a=>console.error("Unable to load consent script URLs, Error",JSON.stringify(a)))})();
